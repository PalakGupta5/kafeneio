                      <input type="text" class="form-control" type="text"  name="pancard" placeholder="Enter Your Pan Card Number" required>
                    </div>
                                              
                    </div>
                  </div>
                   <div class="control-group">
                  <div class="col-md-3">
                    <label for="normal-field" class="control-label">Applied For PanCard</label>
                    </div>
                    <div class="col-md-9">
                    <div class="form-group">
                      <input type="checkbox" value="" name="pancardFlag" class="checkbox"  required>
                    </div>
                                              
                    </div>
                  </div>
                  
                  
                  
                    
                    <div class="control-group">
                  <div class="col-md-3">
                    <label for="normal-field" class="control-label">Enter Sponsor Id</label>
