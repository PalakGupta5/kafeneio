                <div class="form-actions">
                  <div>
                    <input type="submit" class="btn btn-primary"  value="Sign Up"/>
                    <button class="btn btn-default" type="button">Cancel</button>
                  </div>
                </div>
         
            </div>
          </div>
          </div>
          </div> 
</form>
