									<input type="text" name="name" placeholder="Enter Your Name Here" class="form-control" id="name" required>
									</div>
									
							</div>
                  </div>
                  
                  <div class="control-group">
                  <div class="col-md-3">
                    <label for="prepended-input" class="control-label">Email</label>
                    </div>
                    <div class="col-md-9">
								<div class="form-group">
									<div class="input-group">
										<span class="input-group-addon"><i class="icon-user"></i></span>
										<input type="text" class="form-control"
											 name="email"
											id="email" placeholder="Enter Your Email Address Here"
											required>

									</div>
									<div class="errorText">
										
									</div>
								</div>
							</div>
                  </div>
                  
                  
                  
                  
                  
                    <div class="control-group">
                  <div class="col-md-3">
                    <label for="normal-field" class="control-label">Contact Number</label>
                    </div>
