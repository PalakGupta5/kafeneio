                      <div class="input-group"> <span class="input-group-addon"><i class="icon-lock"></i></span>
                        <input type="password"  class="form-control"   name="password" placeholder="Enter Your Password Here" required>
                      </div>
                      <div class="errorText" >
                        
                                  </div>
                      </div>
                    </div>
